// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output="./generator/client_sqlite"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL_TRANSACTIONAL")
}

model Post {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String
  password  String
}
